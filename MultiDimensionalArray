public class multidimentional {
    public static void main(String[]args){
        
        String pres[] [] = {{"Donald","Trump"},
                         {"Digo","D30"},
                         {"Vladimir","Putin","Russia"}
        };
        for(int r = 0; r < pres.length; r++){
            for(int c = 0; c < pres[r].length; c++){
                System.out.println(pres[r][c]);
            }
        System.out.println("");
    }
  }
}
